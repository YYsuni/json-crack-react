{
  "name": "json-crack-react",
  "version": "0.0.0",
  "description": "Simple JSON crack graph react function component.",
  "main": "src/JsonCrack.tsx",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "npm run clean",
